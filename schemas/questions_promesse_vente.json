{
  "titre": "Questions pour Promesse Unilatérale de Vente - Lots de Copropriété",
  "description": "Questions structurées à poser au notaire pour collecter les informations nécessaires à la rédaction d'une promesse unilatérale de vente de lots de copropriété",
  "sections": {
    "1_acte": {
      "titre": "Informations sur l'acte",
      "questions": [
        {
          "id": "acte_date",
          "question": "Date prévue de signature de la promesse ?",
          "type": "date",
          "obligatoire": true,
          "variable": "acte.date"
        },
        {
          "id": "acte_reference",
          "question": "Référence interne du dossier ?",
          "type": "texte",
          "obligatoire": false,
          "variable": "acte.reference_interne"
        },
        {
          "id": "notaire_assistant",
          "question": "Y a-t-il un notaire assistant le bénéficiaire ?",
          "type": "booleen",
          "obligatoire": true,
          "sous_questions": {
            "si_oui": [
              {
                "id": "notaire_beneficiaire_nom",
                "question": "Nom du notaire assistant ?",
                "type": "texte",
                "variable": "acte.notaire_assistant.nom"
              },
              {
                "id": "notaire_beneficiaire_ville",
                "question": "Ville du notaire assistant ?",
                "type": "texte",
                "variable": "acte.notaire_assistant.ville"
              }
            ]
          }
        }
      ]
    },

    "2_promettant": {
      "titre": "Identification du PROMETTANT (vendeur)",
      "questions": [
        {
          "id": "promettant_nombre",
          "question": "Combien de promettants (vendeurs) ?",
          "type": "nombre",
          "obligatoire": true,
          "minimum": 1
        },
        {
          "id": "promettant_civilite",
          "question": "Civilité du promettant ?",
          "type": "choix",
          "options": ["Monsieur", "Madame"],
          "obligatoire": true,
          "variable": "promettant[].civilite",
          "repeter": "par_promettant"
        },
        {
          "id": "promettant_nom",
          "question": "Nom de famille du promettant ?",
          "type": "texte",
          "obligatoire": true,
          "variable": "promettant[].nom",
          "repeter": "par_promettant"
        },
        {
          "id": "promettant_prenoms",
          "question": "Tous les prénoms du promettant ?",
          "type": "texte",
          "obligatoire": true,
          "variable": "promettant[].prenoms",
          "repeter": "par_promettant"
        },
        {
          "id": "promettant_profession",
          "question": "Profession du promettant ?",
          "type": "texte",
          "obligatoire": true,
          "variable": "promettant[].profession",
          "repeter": "par_promettant"
        },
        {
          "id": "promettant_adresse",
          "question": "Adresse complète du promettant ?",
          "type": "texte",
          "obligatoire": true,
          "variable": "promettant[].adresse",
          "repeter": "par_promettant"
        },
        {
          "id": "promettant_naissance_date",
          "question": "Date de naissance du promettant ?",
          "type": "date",
          "obligatoire": true,
          "variable": "promettant[].date_naissance",
          "repeter": "par_promettant"
        },
        {
          "id": "promettant_naissance_lieu",
          "question": "Lieu de naissance du promettant (ville + département/pays) ?",
          "type": "texte",
          "obligatoire": true,
          "variable": "promettant[].lieu_naissance",
          "repeter": "par_promettant"
        },
        {
          "id": "promettant_nationalite",
          "question": "Nationalité du promettant ?",
          "type": "texte",
          "obligatoire": true,
          "variable": "promettant[].nationalite",
          "defaut": "française",
          "repeter": "par_promettant"
        },
        {
          "id": "promettant_situation_matrimoniale",
          "question": "Situation matrimoniale du promettant ?",
          "type": "choix",
          "options": ["Célibataire", "Marié(e)", "Pacsé(e)", "Divorcé(e)", "Veuf/Veuve", "Séparé(e)"],
          "obligatoire": true,
          "variable": "promettant[].situation_matrimoniale.statut",
          "repeter": "par_promettant",
          "sous_questions": {
            "si_marie": [
              {
                "id": "promettant_regime",
                "question": "Régime matrimonial ?",
                "type": "choix",
                "options": ["Communauté légale", "Communauté universelle", "Séparation de biens", "Participation aux acquêts"],
                "variable": "promettant[].situation_matrimoniale.regime_matrimonial"
              },
              {
                "id": "promettant_contrat_mariage",
                "question": "Y a-t-il un contrat de mariage ?",
                "type": "booleen",
                "sous_questions": {
                  "si_oui": [
                    {
                      "id": "promettant_contrat_notaire",
                      "question": "Notaire ayant reçu le contrat ?",
                      "variable": "promettant[].situation_matrimoniale.contrat_mariage.notaire"
                    },
                    {
                      "id": "promettant_contrat_date",
                      "question": "Date du contrat de mariage ?",
                      "variable": "promettant[].situation_matrimoniale.contrat_mariage.date"
                    }
                  ]
                }
              },
              {
                "id": "promettant_conjoint_intervient",
                "question": "Le conjoint intervient-il à l'acte ?",
                "type": "booleen",
                "note": "Obligatoire si bien commun"
              }
            ],
            "si_pacse": [
              {
                "id": "promettant_pacs_date",
                "question": "Date du PACS ?",
                "variable": "promettant[].situation_matrimoniale.pacs.date"
              },
              {
                "id": "promettant_pacs_regime",
                "question": "Régime du PACS ?",
                "type": "choix",
                "options": ["Séparation de biens", "Indivision"],
                "variable": "promettant[].situation_matrimoniale.pacs.regime"
              }
            ]
          }
        },
        {
          "id": "promettant_coordonnees",
          "question": "Coordonnées du promettant (téléphone, email) ?",
          "type": "contact",
          "obligatoire": false,
          "variable": "promettant[].coordonnees",
          "repeter": "par_promettant"
        },
        {
          "id": "promettant_presence",
          "question": "Le promettant sera-t-il présent ou représenté ?",
          "type": "choix",
          "options": ["Présent", "Représenté"],
          "obligatoire": true,
          "variable": "promettant[].presence",
          "repeter": "par_promettant"
        }
      ]
    },

